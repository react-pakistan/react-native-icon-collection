{
  "name": "react-native-commons-collection",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "android": "yarn prestorybook && react-native run-android",
    "build": "yarn clean && yarn build:ts && cp package.json lib && cp README.md lib",
    "build:ts": "tsc --project tsconfig.build.json",
    "clean": "yarn rimraf ./lib",
    "generate-icons": "scripts/generate-icons.sh",
    "ios": "yarn prestorybook && react-native run-ios",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint 'src/**/*.js' --config stylelint.config.js",
    "lint:js": "eslint --config eslint.config.js . *.config.js",
    "lint:ts": "tslint --project tsconfig.json",
    "prestorybook": "rnstl",
    "start": "react-native start",
    "storybook": "start-storybook -p=9002",
    "test": "jest",
    "upgrade": "ncu -u"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-native": "0.61.5",
    "react-native-svg": "^11.0.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@react-pakistan/react-commons-collection": "^3.0.0",
    "@storybook/addon-actions": "^5.3.11",
    "@storybook/addon-info": "^5.3.11",
    "@storybook/addon-knobs": "^5.3.11",
    "@storybook/addon-ondevice-actions": "^5.3.11",
    "@storybook/addon-ondevice-knobs": "^5.3.11",
    "@storybook/react-native": "^5.3.11",
    "@storybook/react-native-server": "^5.3.11",
    "@types/jest": "^25.1.1",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.10",
    "@types/react-test-renderer": "^16.9.2",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^4.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.12.0",
    "stylelint": "^13.0.0",
    "stylelint-config-react-native-styled-components": "^0.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "stylelint-react-native": "^2.1.1",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Taimoormk/react-native-commons-collection.git"
  },
  "author": "Taimoor Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Taimoormk/react-native-commons-collection/issues"
  },
  "homepage": "https://github.com/Taimoormk/react-native-commons-collection#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./__stories__/story"
      ],
      "pattern": "*.story.tsx",
      "outputFile": "./__stories__/story-loader.ts"
    }
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6",
    "yarn": "*"
  }
}
